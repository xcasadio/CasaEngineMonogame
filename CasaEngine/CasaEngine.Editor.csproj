<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>net6.0-windows</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<UseWindowsForms>true</UseWindowsForms>
	</PropertyGroup>
	<PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|x86' ">
		<DefineConstants>TRACE;DEBUG;WINDOWS;EDITOR</DefineConstants>
	</PropertyGroup>
	<PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|x86' ">
		<DefineConstants>TRACE;WINDOWS;EDITOR</DefineConstants>
	</PropertyGroup>
	<PropertyGroup Condition="'$(Configuration)|$(Platform)' == 'Debug UnitTest|x86'">
		<DefineConstants>TRACE;DEBUG;WINDOWS EDITOR UNITTEST</DefineConstants>
	</PropertyGroup>
	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
		<DefineConstants>$(DefineConstants)TRACE;EDITOR</DefineConstants>
	</PropertyGroup>
	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
		<DefineConstants>$(DefineConstants)TRACE;EDITOR</DefineConstants>
	</PropertyGroup>
	<ItemGroup>
	  <PackageReference Include="Microsoft.Build" Version="17.4.0" />
	</ItemGroup>
	<ItemGroup>
		<ProjectReference Include="..\CasaEngineCommon\CasaEngineCommon.Editor.csproj" />
		<ProjectReference Include="..\FarseerPhysics\Farseer Physics XNA.csproj" />
	</ItemGroup>
	<ItemGroup>
		<Reference Include="FNA">
			<HintPath>..\ThirdParties\FNA\Release\FNA.dll</HintPath>
		</Reference>
		<Reference Include="P4API">
		  <HintPath>..\ThirdParties\P4.Net\P4API.dll</HintPath>
		</Reference>
	</ItemGroup>
</Project>