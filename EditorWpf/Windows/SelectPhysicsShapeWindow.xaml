<Window x:Class="EditorWpf.Windows.SelectPhysicsShapeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EditorWpf.Windows"
        mc:Ignorable="d"
        Title="Select Physics Shape" Height="150" Width="200">
    <StackPanel>
        <Button Click="ButtonBox_Click">Box</Button>
        <Button Click="ButtonSphere_Click">Sphere</Button>
        <Button Click="ButtonCylinder_Click">Cylinder</Button>
        <Button Click="ButtonCapsule_Click">Capsule</Button>
        <Button Click="ButtonCompound_Click">Compound</Button>
    </StackPanel>
</Window>
