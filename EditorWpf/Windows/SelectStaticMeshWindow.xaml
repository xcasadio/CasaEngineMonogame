<Window x:Class="EditorWpf.Windows.SelectStaticMeshWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Select Static Mesh" Height="191" Width="250">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="30"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>

        <StackPanel Grid.Row="0">
            <Button Click="ButtonCube_Click">Cube</Button>
            <Button Click="ButtonSphere_Click">Sphere</Button>
            <Button Click="ButtonCapsule_Click">Capsule</Button>
            <Button Click="ButtonCylinder_Click">Cylinder</Button>
            <Button Click="ButtonPlane_Click">Plane</Button>
            <Button Click="ButtonTeapot_Click">Teapot</Button>
            <Button Click="ButtonTorus_Click">Torus</Button>
        </StackPanel>

        <Button Grid.Row="2" Click="ButtonCancel_Click" HorizontalAlignment="Right" Width="100">Cancel</Button>
    </Grid>
</Window>
